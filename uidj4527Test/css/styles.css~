/********* LIST */
ol {
    counter-reset:li; /* Initiate a counter */
    margin-left:40px; /* Remove the default left margin */ 
}

ol > li:before {
    content:counter(li, decimal-leading-zero); /* Use the counter as content */
    counter-increment:li; /* Increment the counter by 1 */
    color:#3699b9;
}
